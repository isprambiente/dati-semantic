###  https://w3id.org/italia/env/onto/inspire-mf/basedOnMetric
:basedOnMetric rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf top:hasDescription ;
               owl:inverseOf :isMetricUsedBy ;
               rdfs:domain top:Entity ;
               rdfs:range :Metric ;
               rdfs:comment "La relazione tra un'entità e la metrica utilizzata per eseguire la misurazione."@it ,
                            "The relation between an entity and the metric used for performing the measurement."@en ;
               rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
               rdfs:label "basato sulla metrica"@it ,
                          "based on metric"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/generationTime
:generationTime rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf top:atTime ;
                owl:inverseOf :isGenerationTimeOf ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Observation
                                            :Sampling
                                          )
                            ] ;
                rdfs:range top:TemporalEntity ;
                rdfs:comment "La relazione che sussiste tra un'osservazione (o un campionamento) e l'entità temporale in cui essa è stata generata."@it ,
                             "The relation holding between an observation (or a sampling) and its generation time."@en ;
                rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                rdfs:label "entità temporale di generazione dell'osservazione (o del campionamento)"@it ,
                           "temporal entity of observation (or sampling) generation"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasFeatureOfInterest
:hasFeatureOfInterest rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf top:associatedWith ;
                      owl:inverseOf :isFeatureOfInterestOf ;
                      rdfs:domain top:Entity ;
                      rdfs:range :FeatureOfInterest ;
                      rdfs:comment "La relazione esistente tra una qualsiasi entità ed una caratteristica di interesse."@it ,
                                   "The relationship between any entity and a feature of interest."@en ;
                      rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                      rdfs:label "ha caratteristica di interesse"@it ,
                                 "has feature of interest"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasIndicator
:hasIndicator rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf top:associatedWith ;
              owl:inverseOf :isIndicatorOf ;
              rdfs:domain top:Entity ;
              rdfs:range :Indicator ;
              rdfs:comment "La relazione tra un'entità ed un indicatore."@it ,
                           "The relation between an entity and an indicator."@en ;
              rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
              rdfs:label "ha indicatore"@it ,
                         "has indicator"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasManufacturer
:hasManufacturer rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf top:associatedWith ;
                 rdfs:domain top:Entity ;
                 rdfs:range top:Organisation ;
                 rdfs:comment "Property used to link the platform model to its manufacturer (intended as an organisation)"@en ,
                              "Proprietà utilizzata per collegare il modello della piattaforma al relativo costruttore (inteso come organizzazione)"@it ;
                 rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                 rdfs:label "ha costruttore"@it ,
                            "has manufacturer"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasObservationParameter
:hasObservationParameter rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf top:isParametrisedBy ;
                         owl:inverseOf :isObservationParameterFor ;
                         rdfs:domain :Observation ;
                         rdfs:range :ObservationParameter ;
                         rdfs:comment "Property that links an observation to its parameter being observed."@en ,
                                      "Proprietà che collega un'osservazione al suo parametro osservato."@it ;
                         rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                         rdfs:label "ha parametro di osservazione"@it ,
                                    "has observation parameter"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasObservationValue
:hasObservationValue rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf top:hasValue ;
                     owl:inverseOf :isObservationValueOf ;
                     rdfs:domain :Observation ;
                     rdfs:range :ObservationValue ;
                     rdfs:comment "La proprietà che collega un'osservazione al suo valore."@it ,
                                  "The property that links an observation to its value."@en ;
                     rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                     rdfs:label "ha valore di osservazione"@it ,
                                "has observation value"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasOperationalStatus
:hasOperationalStatus rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf top:associatedWith ;
                      rdfs:domain :Platform ;
                      rdfs:range top:Concept ;
                      rdfs:comment "Property used to link the platform at its operational status."@en ,
                                   "Proprietà utilizzata per collegare lo stato operativo alla piattaforma"@it ;
                      rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                      rdfs:label "ha stato operativo"@it ,
                                 "has operational status"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasPlatform
:hasPlatform rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf top:associatedWith ;
             owl:inverseOf :isPlatformOf ;
             rdfs:domain top:Entity ;
             rdfs:range :Platform ;
             rdfs:comment "La relazione tra un'entità e una piattaforma."@it ,
                          "The relation between an entity and a platform."@en ;
             rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
             rdfs:label "ha piattaforma"@it ,
                        "has platform"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasPlatformModel
:hasPlatformModel rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf top:hasDescription ;
                  owl:inverseOf :isPlatformModelOf ;
                  rdfs:domain :Platform ;
                  rdfs:range :PlatformModel ;
                  rdfs:comment "La proprietà che consente di associare una piattaforma al modello di piattaforma corrispondente."@it ,
                               "The property that allows one to associate a platform with its corresponding platform model."@en ;
                  rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                  rdfs:label "ha modello di piattaforma"@it ,
                             "has platform model"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasPlatformType
:hasPlatformType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf top:isClassifiedBy ;
                 owl:inverseOf :isPlatformTypeOf ;
                 rdfs:domain :Platform ;
                 rdfs:range :PlatformType ;
                 rdfs:comment "La proprietà che consente di classificare un piattaforma utilizzando il concetto rappresentato dal tipo di piattaforma."@it ,
                              "The property that allows you to classify a platform using the concept represented by the type of platform."@en ;
                 rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                 rdfs:label "ha tipo di piattaforma"@it ,
                            "has platform type"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasResponsibleParty
:hasResponsibleParty rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf top:associatedWith ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( :MonitoringFacility
                                                 :MonitoringNetwork
                                               )
                                 ] ;
                     rdfs:range top:Organisation ;
                     rdfs:comment "The relation holding between a monitiroring facility or network and the agent is in charge of some responsibility role." ;
                     rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                     rdfs:label "ha parte responsabile"@it ,
                                "has responsible party"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasSamplerModel
:hasSamplerModel rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf top:hasDescription ;
                 owl:inverseOf :isSamplerModelOf ;
                 rdfs:domain :Sampler ;
                 rdfs:range :SamplerModel ;
                 rdfs:comment "La relazione tra un campionatore e la descrizione del campionatore rappresentata dal suo modello."@it ,
                              "The relation between a sampler and the sampler description represented by its model."@en ;
                 rdfs:isDefinedBy <https://w3id.org/italia/env/onto/inspire-mf> ;
                 rdfs:label "ha modello di campionatore"@it ,
                            "has sampler model"@en .


###  https://w3id.org/italia/env/onto/inspire-mf/hasSamplerType
:hasSamplerType rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf top:isClassifiedBy ;
                owl:inverseOf :isSamplerTypeOf ;
                rdfs:domain :Sampler ;
