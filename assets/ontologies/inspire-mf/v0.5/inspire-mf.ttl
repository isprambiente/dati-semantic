                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasSensorType ;
                          owl:someValuesFrom :SensorType
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :isHostedBy ;
