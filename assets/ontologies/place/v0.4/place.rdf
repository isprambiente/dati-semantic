        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/env/onto/place"/>
        <rdfs:label xml:lang="en">is direct lower rank of</rdfs:label>
        <rdfs:label xml:lang="it">è il livello diretto più basso di</rdfs:label>
    </owl:ObjectProperty>
    


