#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/italia/env/onto/top/associatedWith
:associatedWith rdf:type owl:ObjectProperty ;
                rdfs:comment "La relazione più generica nella rete di ontologie ISPRA."@it ,
                             "The most generic relation that can be used within the ISPRA ontonet."@en ;
                rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                rdfs:label "associated with"@en ,
                           "associato a"@it .


###  https://w3id.org/italia/env/onto/top/atTime
:atTime rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :associatedWith ;
        owl:inverseOf :isTimeOf ;
        rdfs:domain :Entity ;
        rdfs:range :TemporalEntity ;
        rdfs:comment "La relazione esistente tra una qualsiasi entità ed un'entità temporale."@it ,
                     "The relation holding between an entity ad a temporal entity."@en ;
        rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
        rdfs:label "al tempo"@it ,
                   "at time"@en .


###  https://w3id.org/italia/env/onto/top/classifies
:classifies rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :associatedWith ;
            owl:inverseOf :isClassifiedBy ;
            rdfs:domain :Concept ;
            rdfs:range :Entity ;
            rdfs:comment "La proprietà utilizzata per affermare che un concetto classificava una certa entità."@it ,
                         "The property used to state that a concept classified a certain entity."@en ;
            rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
            rdfs:label "classifica"@it ,
                       "classifies"@en .


###  https://w3id.org/italia/env/onto/top/directlyFollows
:directlyFollows rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :follows ;
                 owl:inverseOf :directlyPrecedes ;
                 rdfs:domain :Entity ;
                 rdfs:range :Entity ;
                 rdfs:comment "L'intransitivo segue la relazione. Ad esempio, il martedì segue direttamente il lunedì. L'immediatezza della precedenza dipende dalla concettualizzazione del progettista."@it ,
                              "The intransitive follows relation. For example, Tuesday directly follows Monday. Directness of precedence depends on the designer conceptualization."@en ;
                 rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                 rdfs:label "directly follows"@en ,
                            "segue direttamente"@it .


###  https://w3id.org/italia/env/onto/top/directlyPrecedes
:directlyPrecedes rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :precedes ;
                  rdfs:domain :Entity ;
                  rdfs:range :Entity ;
                  rdfs:comment "L'intransitivo precede la relazione. Ad esempio, il lunedì precede direttamente il martedì. L'immediatezza della precedenza dipende dalla concettualizzazione del progettista."@it ,
                               "The intransitive precedes relation. For example, Monday directly precedes Tuesday. Directness of precedence depends on the designer conceptualization."@en ;
                  rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                  rdfs:label "directly precedes"@en ,
                             "precede direttamente"@it .


###  https://w3id.org/italia/env/onto/top/follows
:follows rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :associatedWith ;
         owl:inverseOf :precedes ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :Entity ;
         rdfs:range :Entity ;
         rdfs:comment "A relation between entities, expressing a 'sequence' schema."@en ,
                      "Una relazione tra entità, che esprime uno schema di 'sequenza'."@it ;
         rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
         rdfs:label "follows"@en ,
                    "segue"@it .


###  https://w3id.org/italia/env/onto/top/hasAgentRole
:hasAgentRole rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasEventuality ;
              owl:inverseOf :isAgentRoleOf ;
              rdfs:domain :Entity ;
              rdfs:range :AgentRole ;
              rdfs:comment "La relazione che permette di collegare un'entità alla relazione n-aria che rappresenta il ruolo ricoperto da un agente."@it ,
                           "The relationship that allows you to connect an entity to the n-aria relationship that represents the role played by an agent."@en ;
              rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
              rdfs:label "ha ruolo-agente"@it ,
                         "has agent-role"@en .


###  https://w3id.org/italia/env/onto/top/hasBroader
:hasBroader rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :associatedWith ;
            owl:inverseOf :hasNarrower ;
            rdf:type owl:TransitiveProperty ;
            rdfs:domain :Concept ;
            rdfs:range :Concept ;
            rdfs:comment "La relazione per creare tassonomie di concetti dal più specifico al più generale."@it ,
                         "The relation for creating hierarchies of concepts from the most specific to the most general."@en ;
            rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
            rdfs:label "ha più generale"@it ,
                       "has broader"@en .


###  https://w3id.org/italia/env/onto/top/hasCharacteristic
:hasCharacteristic rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :associatedWith ;
                   owl:inverseOf :isCharacteristicOf ;
                   rdfs:domain :Entity ;
                   rdfs:range :Characteristic ;
                   rdfs:comment "La relazione di primo livello che intercorre tra un'entità e una delle sue caratteristiche."@it ,
                                "The top-level relation holding between an entity and one of its characteristics."@en ;
                   rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                   rdfs:label "ha caratteristica"@it ,
                              "has characteristic"@en .


###  https://w3id.org/italia/env/onto/top/hasCollection
:hasCollection rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :associatedWith ;
               owl:inverseOf :isCollectionOf ;
               rdfs:domain :Entity ;
               rdfs:range :Collection ;
               rdfs:comment "La relazione esistente tra un'entità ed una collezione."@it ,
                            "The relation holding between an entity and a collection."@en ;
               rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
               rdfs:label "ha collezione"@it ,
                          "has collection"@en .


###  https://w3id.org/italia/env/onto/top/hasCreator
:hasCreator rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :associatedWith ;
            owl:inverseOf :isCreatorOf ;
            rdfs:domain :Entity ;
            rdfs:range :Agent ;
            rdfs:comment "Relazione per associare ad un'entità un agente creatore."@it ,
                         "The relation for associating an entity to an agent being the creator."@en ;
            rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
            rdfs:label "ha creatore"@it ,
                       "has creator"@en .


###  https://w3id.org/italia/env/onto/top/hasDataSchema
:hasDataSchema rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasDescription ;
               owl:inverseOf :isDataSchemaOf ;
               rdfs:domain :Media ;
               rdfs:range :DataSchema ;
               rdfs:comment "La relazione che sussiste tra un media e il suo schema di dati."@it ,
                            "The relation holding between a media and its data schema."@en ;
               rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
               rdfs:label "ha schema di dati"@it ,
                          "has data schema"@en .


###  https://w3id.org/italia/env/onto/top/hasDescription
:hasDescription rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :associatedWith ;
                owl:inverseOf :isDescriptionOf ;
                rdfs:domain :Entity ;
                rdfs:range :Description ;
                rdfs:comment "Property that associates an entity with its description."@en ,
                             "Proprietà che associa un'entità alla sua descrizione."@it ;
                rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                rdfs:label "ha descrizione"@it ,
                           "has description"@en .


###  https://w3id.org/italia/env/onto/top/hasDownloadURL
:hasDownloadURL rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :associatedWith ;
                rdfs:domain :Media ;
                rdfs:comment "Questo è un link diretto al file scaricabile in un dato formato. E.g. un file CSV o un file RDF. Il formato è descritto dal dct:format e/o dal dcat:mediaType della distribuzione."@it ,
                             "The URL of the downloadable file in a given format. E.g. CSV file or RDF file. The format is indicated by the distribution's dct:format and/or dcat:mediaType."@en ;
                rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                rdfs:label "ha URL di scaricamento"@it ,
                           "has download URL"@en .


###  https://w3id.org/italia/env/onto/top/hasEventuality
:hasEventuality rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :associatedWith ;
                owl:inverseOf :isEventualityOf ;
                rdfs:domain :Entity ;
                rdfs:range :Eventuality ;
                rdfs:comment "La relazione esistente tra una qualsiasi entità ed una di tipo Eventualità."@it ,
                             "The relationship existing between any entity and another typed as Eventuality."@en ;
                rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                rdfs:label "ha eventualità"@it ,
                           "has eventuality"@en .


###  https://w3id.org/italia/env/onto/top/hasIdentifierSchema
:hasIdentifierSchema rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasDescription ;
                     owl:inverseOf :isIdentifierSchemaOf ;
                     rdfs:domain :UniqueIdentifier ;
                     rdfs:range :IdentifierSchema ;
                     rdfs:comment "La relazione che lega un identificativo unico al suo schema di definizione."@it ,
                                  "The relation that associates a unique identifier to its defining schema."@en ;
                     rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
                     rdfs:label "ha schema di identificazione"@it ,
                                "has identifier schema"@en .


###  https://w3id.org/italia/env/onto/top/hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :associatedWith ;
             owl:inverseOf :isLocationOf ;
             rdfs:domain :Entity ;
             rdfs:range :Location ;
             rdfs:comment "La relazione esistente tra un qualsiasi tipo di entità ed un luogo."@it ,
                          "The relation holding between any kind of entity and a place."@en ;
             rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
             rdfs:label "ha luogo"@it ,
                        "has place"@en .


###  https://w3id.org/italia/env/onto/top/hasMedia
:hasMedia rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :associatedWith ;
          owl:inverseOf :isMediaOf ;
          rdfs:domain :Entity ;
          rdfs:range :Media ;
          rdfs:comment "La relazione tra un'entità ed un'istanza della classe Media."@it ,
                       "The relationship between an entity and an instance of the middle class."@en ;
          rdfs:isDefinedBy <https://w3id.org/italia/env/onto/top> ;
          rdfs:label "ha media"@it ,
                     "has media"@en .


###  https://w3id.org/italia/env/onto/top/hasMember
